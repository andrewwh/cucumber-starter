{
  "name": "cucumber-starter",
  "version": "1.0.0",
  "description": "Cucumber starter",
  "main": "out/index.js",
  "scripts": {
    "start": "node out/index.js",
    "build": "tsc",
    "clean": "rm -rf out/*; rm -f tsconfig.tsbuildinfo",
    "lint:gherkin": "gherkin-lint -c ./.gherkin-lintrc ./features/**",
    "lint:ts": "eslint src --ext .ts,.tsx",
    "test": "tsc && node out/index.js"
  },
  "dependencies": {
    "@cucumber/cucumber": "7.0.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-match": "^1.1.1",
    "chai-string": "1.5.0",
    "chromedriver": "87.0.0",
    "commander": "2.20.0",
    "fs-plus": "3.1.1",
    "geckodriver": "^1.19.1",
    "gherkin-lint": "^4.1.3",
    "http": "0.0.1-security",
    "path": "0.12.7",
    "require-dir": "1.2.0",
    "selenium-webdriver": "^4.0.0-alpha.8"
  },
  "author": "Andrew Wheeler",
  "license": "BSD",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/commander": "^2.12.2",
    "@types/fs-plus": "^3.0.1",
    "@types/node": "^12.19.14",
    "@types/selenium-webdriver": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "typescript": "^4.1.3"
  }
}
